{
  "name": "shared-types",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared-types/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:types"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/libs/shared-types"],
      "options": {
        "command": "tsc -p tsconfig.lib.json --outDir ../../dist/libs/shared-types",
        "cwd": "libs/shared-types"
      }
    },
    "generate-types": {
      "executor": "nx:run-commands",
      "options": {
        "command": "openapi-generator-cli generate -i tools/openapi/schema.json -g typescript-fetch -o libs/shared-types/src/generated --additional-properties=typescriptThreePlus=true,supportsES6=true,npmName=@camera-store/shared-types,npmVersion=1.0.0"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'Linting shared-types - no linter configured'",
        "cwd": "libs/shared-types"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{projectRoot}/coverage"],
      "options": {
        "jestConfig": "libs/shared-types/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}