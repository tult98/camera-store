{
  "name": "api-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api-client/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:client"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/libs/api-client"],
      "options": {
        "command": "tsc -p tsconfig.lib.json --outDir ../../dist/libs/api-client",
        "cwd": "libs/api-client"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'Linting api-client - no linter configured'",
        "cwd": "libs/api-client"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{projectRoot}/coverage"],
      "options": {
        "jestConfig": "libs/api-client/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "implicitDependencies": ["shared-types"]
}